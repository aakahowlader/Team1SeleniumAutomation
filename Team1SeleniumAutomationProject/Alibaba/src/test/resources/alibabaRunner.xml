<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite name="Amazon Automation Script " parallel="false">

    <parameter name="useCloudEnv" value="false"/>
    <parameter name="cloudEnvName" value="browserstack"/>
    <parameter name="os" value="Windows"/>
    <parameter name="os_version" value="Windows 10"/>
    <parameter name="browserName" value="chrome"/>
    <parameter name="browserVersion" value="75.0"/>
    <parameter name="url" value="https://www.alibaba.com"/>

    <test name = "Alibaba Search">
        <classes>
            <class name="home.AlibabaSearch"/>
            <class name="search.TestSearch"/>
        </classes>
    </test>

</suite>
